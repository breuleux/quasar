
require:
   .util ->
      ID, methods
   .proxy ->
      deprox
   kaiser ->
      maybe-handler
      default-serializer as kser

provide:
   ;; iterate-properties
   remap-properties


;; iterate-properties{match obj} =
;;    null? or undefined? ->
;;       {}
;;    else ->
;;       deprox{obj}[methods.iterate-properties]{}


remap-properties{value, fn} =
   h = maybe-handler{value}
   if not h:
      return null
   var changes = false
   s = kser.to-object{value, false}.data
   items{s} each {k, v} ->
      newv = fn{k, v}
      if newv !== v:
         changes = true
         s[k] = newv
   if changes:
      h.fill{value, s}


